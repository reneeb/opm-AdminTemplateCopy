<?xml version="1.0" encoding="utf-8" ?>
<otrs_config version="1.0" init="Config">
    <ConfigItem Name="Frontend::Output::FilterElementPost###AdminTemplateCopy" Required="0" Valid="1">
        <Description Translatable="1">Adds a link to copy a template in the admin template overview.</Description>
        <Group>AdminTemplateCopy</Group>
        <SubGroup>OutputFilter</SubGroup>
        <Setting>
            <Hash>
                <Item Key="Module">Kernel::Output::HTML::FilterElementPost::AdminTemplateCopy</Item>
                <Item Key="Templates">
                    <Hash>
                        <Item Key="AdminTemplate">1</Item>
                    </Hash>
                </Item>
            </Hash>
        </Setting>
    </ConfigItem>
    <ConfigItem Name="Frontend::Module###AdminTemplateCopy" Required="0" Valid="1">
        <Description Lang="en">Frontend module registration for template copy module.</Description>
        <Description Lang="de">Frontendmodul-Registration f√ºr das AdminTemplateCopy-Modul.</Description>
        <Group>AdminTemplateCopy</Group>
        <SubGroup>Frontend::Admin::ModuleRegistration</SubGroup>
        <Setting>
            <FrontendModuleReg>
                <Description>Copy templates.</Description>
                <NavBarName>Admin</NavBarName>
                <Title>Copy templates.</Title>
            </FrontendModuleReg>
        </Setting>
    </ConfigItem>
</otrs_config>
